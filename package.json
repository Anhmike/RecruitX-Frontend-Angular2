{
  "name": "Recruitx-Frontend-Angular2",
  "version": "1.0.0",
  "description": "Recruitx-Frontend-Angular2",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "start": "npm run server:dev:live",
    "pretest": "npm run lint",
    "test": "karma start",
    "copy:static": "cp -r ../common/images dist/",
    "protractor": "protractor",
    "preuitest": "npm run webdriver:update -- --standalone",
    "uitest": "npm run protractor",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "dev": "npm run build:dev -- --watch",
    "build": "npm run build:dev",
    "build:dev": "webpack --config ./webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:dev": "npm run clean:dist",
    "postbuild:dev": "npm run copy:static",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config ./webpack.config.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:live": "npm run server:dev -- --hot",
    "server:test": "webpack-dev-server --config config/webpack.test.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:test:live": "npm run server:test -- --hot",
    "lint": "npm run tslint src/**/*.ts",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "uitest:live": "npm run uitest -- --elementExplorer",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.2",
    "@angular/compiler": "^2.0.0-rc.2",
    "@angular/core": "^2.0.0-rc.2",
    "@angular/http": "^2.0.0-rc.2",
    "@angular/platform-browser": "^2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.2",
    "@angular/router": "^2.0.0-rc.2",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "ng2-bootstrap": "^1.0.17",
    "ng2-translate": "^2.2.0",
    "normalize.css": "^4.1.1",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^1.0.0",
    "codelyzer": "0.0.23",
    "copy-webpack-plugin": "^3.0.1",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.0",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.3",
    "font-awesome-webpack": "^0.0.4",
    "grunt": "^0.4.1",
    "grunt-angular-templates": "^0.5.7",
    "grunt-artifactory-artifact": "^0.8.0",
    "grunt-autoprefixer": "^0.7.3",
    "grunt-babel": "^5.0.1",
    "grunt-concurrent": "^0.5.0",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-compass": "^0.7.2",
    "grunt-contrib-compress": "0.13.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-connect": "^0.7.1",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-cssmin": "^0.9.0",
    "grunt-contrib-htmlmin": "^0.3.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-symlink": "^0.3.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-env": "^0.4.4",
    "grunt-filerev": "^2.2.0",
    "grunt-githooks": "^0.3.1",
    "grunt-google-cdn": "^0.4.0",
    "grunt-jsonlint": "^1.0.4",
    "grunt-newer": "^0.7.0",
    "grunt-ng-annotate": "^0.3.0",
    "grunt-preprocess": "^4.1.0",
    "grunt-protractor-runner": "^2.1.0",
    "grunt-remfallback": "~0.0.5",
    "grunt-shell-spawn": "^0.3.10",
    "grunt-svgmin": "^0.4.0",
    "grunt-text-replace": "^0.4.0",
    "grunt-usemin": "^3.0.0",
    "grunt-webpack": "^1.0.11",
    "grunt-wiredep": "^1.7.0",
    "grunt-karma": "^0.9.0",
    "grunt-typings": "^0.1.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.12.23",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.1.8",
    "karma-jasmine-html-reporter-livereload": "~1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "lodash": "^4.13.1",
    "ng2-cookies": "^0.1.9",
    "protractor": "^3.2.2",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.3",
    "request": "^2.60.0",
    "rimraf": "^2.5.2",
    "simple-fmt": "^0.1.0",
    "source-map-loader": "^0.1.5",
    "ts-node": "^0.9.0",
    "tslint": "^3.7.1",
    "typedoc": "^0.4.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
